              "pattern_name": "string",
              "description": "string",
              "involved_levels": ["string"],
              "supporting_evidence": ["string"],
              "significance": "number"
            }
          ],
          "causal_relationships": [
            {
              "cause_level": "string",
              "cause_factor": "string",
              "effect_level": "string",
              "effect_factor": "string",
              "relationship_strength": "number",
              "confidence": "number"
            }
          ],
          "system_insights": [
            {
              "insight": "string",
              "derived_from": ["string"],
              "actionability": "number",
              "potential_impact": "number"
            }
          ]
        }
      }
    }
  }
